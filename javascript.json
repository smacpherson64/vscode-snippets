{
  // Place your snippets for javascriptreact here. Each snippet is defined under a snippet name and has a prefix, body and
  // description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
  // $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
  // same ids are connected.
  // Example:
  "Code Heading": {
    "prefix": ["code-heading-main", "// ==========================="],
    "body": [
      "// ===========================",
      "// $1",
      "// ===========================",
      ""
    ],
    "description": "Creates a Code Headding"
  },
  "Code Heading Sub": {
    "prefix": ["code-sub-heading", "// ---------------------------"],
    "body": [
      "// ---------------------------",
      "// $1",
      "// ---------------------------",
      ""
    ],
    "description": "Creates a Code Headding"
  },
  "Console": {
    "prefix": ["console.log"],
    "body": ["console.log($1);"],
    "description": "Log output to console"
  },
  "Jest Init Test": {
    "prefix": "jest-init-test",
    "body": [
      "import { ${1:name} } from \"./$2\";",
      "",
      "describe(\"${1:name}\", () => {",
      "  it(\"$3\", () => {",
      "    $4",
      "  });",
      "});",
      ""
    ],
    "description": "Jest Init Test"
  },
  "Jest Describe": {
    "prefix": "describe",
    "body": [
      "describe(\"${1:name}\", () => {",
      "  it(\"$2\", () => {",
      "    $3",
      "  });",
      "});",
      ""
    ],
    "description": "Jest Describe"
  },
  "Jest It": {
    "prefix": "it",
    "body": ["it(\"$2\", () => {", "  $3", "});", "", ""],
    "description": "Jest It"
  },
  "Gherkin Capability": {
    "prefix": "capability",
    "body": [
      "In order to ${1:business goal}",
      "As a ${2:internal stakeholder}",
      "I want ${3:stakeholder} to be able to ${4:high level feature}"
    ]
  },
  "Gherkin Feature": {
    "prefix": "feature",
    "body": [
      "#language: en",
      "",
      "Feature: ${1:feature name}",
      "",
      "As a ${1: stakeholder}",
      "I want to be able to ${2: some capability}",
      "So that I can ${3: perform some action}"
    ]
  },
  "Gherkin Example Scenario": {
    "prefix": ["example", "scenerio"],
    "body": [
      "Scenario: ${1:name}",
      "  Given ${2: a stakeholder takes an action}",
      "  When ${3: some event or condition occurs}",
      "  Then ${4: this result should happen}"
    ]
  }
}
